fine_tuned_bert_agnews/*.pt filter=lfs diff=lfs merge=lfs -text
results/**/*.bin filter=lfs diff=lfs merge=lfs -text
results/**/*.safetensors filter=lfs diff=lfs merge=lfs -text
results/**/*.pt filter=lfs diff=lfs merge=lfs -text
# Ensure these files are treated as binary
*.bin binary
